{
  "name": "school-transit",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start dev": "expo start --dev-client",
    "start dev-clean": "expo start --dev-client --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "android device": "expo run:android --device",
    "ios device": "expo run:ios --device",
    "clean ios": "cd ios && pod deintegrate && pod install",
    "clean android": "cd android && ./gradlew clean",
    "web": "expo start --web",
    "prebuild": "npx expo prebuild",
    "podinstall": "npx pod-install ios",
    "prebuild clean": "npx expo prebuild --clean",
    "eas build configure": "eas build:configure",
    "eas update configure": "eas update:configure",
    "fixing expo": "npx expo install --fix",
    "check expo": "npx expo install --check",
    "expo:doctor": "npx expo-doctor",
    "check:login": "eas whoami",
    "login": "eas login",
    "account list": "eas account:list",
    "build:android11and12": "EXPO_ANDROID_REACT_NATIVE_ARCHITECTURES=arm64-v8a eas build --platform android --local --profile production --clear-cache",
    "build:apk": "eas build --platform android --local --profile production --clear-cache",
    "build:eas:apk": "eas build --platform android --profile production --clear-cache",
    "build:androiddev": "eas build --platform android --profile development --clear-cache",
    "build:iosdev": "eas build --platform ios --profile production --clear-cache",
    "build:ios": "eas build --platform ios --local --clear-cache",
    "optimize:app": "rm -rf node_modules && rm -rf .expo && rm -rf yarn.lock && yarn && cd android && ./gradlew clean && cd .. && cd ios && rm -rf Podfile.lock && rm -rf .xcode.env.local && pod install && cd ..",
    "postinstall": "npx tailwindcss -i ./src/assets/style/global.css -o ./node_modules/.cache/nativewind/global.css"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.535.0",
    "@config-plugins/react-native-dynamic-app-icon": "^9.0.0",
    "@expo/vector-icons": "^14.0.4",
    "@gorhom/bottom-sheet": "^5.1.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/hooks": "^3.0.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/luxon": "^3.3.3",
    "@types/redux-logger": "^3.0.13",
    "axios": "1.6.7",
    "crypto-js": "^4.2.0",
    "date-fns": "2.29.3",
    "expo": "~52.0.46",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "~0.13.2",
    "expo-camera": "~16.0.18",
    "expo-checkbox": "~4.0.1",
    "expo-contacts": "~14.0.5",
    "expo-dev-client": "~5.0.20",
    "expo-device": "~7.0.2",
    "expo-document-picker": "~13.0.3",
    "expo-file-system": "~18.0.11",
    "expo-font": "~13.0.3",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-local-authentication": "~15.0.2",
    "expo-location": "~18.0.10",
    "expo-network": "~7.0.5",
    "expo-notifications": "~0.29.14",
    "expo-print": "~14.0.3",
    "expo-router": "^4.0.20",
    "expo-secure-store": "~14.0.1",
    "expo-sharing": "~13.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "~0.27.4",
    "formik": "^2.4.6",
    "lottie-react-native": "^7.2.2",
    "lucide-react": "^0.508.0",
    "nativewind": "4.0.36",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-async-storage": "^0.0.1",
    "react-native-feather": "^1.1.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-gifted-charts": "^1.4.11",
    "react-native-inactivity": "^0.1.8",
    "react-native-mmkv": "^2.12.2",
    "react-native-paystack-webview": "^5.0.1",
    "react-native-raw-bottom-sheet": "^3.0.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-select-dropdown": "^4.0.0",
    "react-native-svg": "15.8.0",
    "react-native-svg-transformer": "^1.5.0",
    "react-native-toast-message": "^2.2.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "13.12.5",
    "react-redux": "^8.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "uuid": "^9.0.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "latest",
    "@react-native/gradle-plugin": "^0.78.2",
    "@types/date-fns": "^2.6.3",
    "@types/react": "~18.3.12",
    "@types/react-native-modern-datepicker": "^1.0.5",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.2",
    "date-parser": "^0.0.7",
    "postcss": "^8.5.2",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    },
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
